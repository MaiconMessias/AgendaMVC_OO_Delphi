unit UntPessoa;

interface

type
  TUtil = class
    class function divisao(const a, b: double): double;
    class function multiplicacao(const a, b: double): double;
  end;

implementation

{ TUtil }

class function TUtil.divisao(const a, b: double): double;
begin
  result := a / b;
end;

class function TUtil.multiplicacao(const a, b: double): double;
begin
  result := a * b;
end;

end.
